  text-align: center;
  width: 650rpx;
  height: 300rpx;
  line-height: 450rpx;
}

.header image {
  width: 200rpx;
  height: 200rpx;
}

.content {
  margin-left: 50rpx;
  margin-bottom: 90rpx;
}

.content text {
  display: block;
  color: #9d9d9d;
  margin-top: 40rpx;
}

.connickname {
  margin-bottom: 90rpx;
  text-align: center;
}

.connickname text {
  display: block;
  color: #000000;
  margin-top: 30rpx;
}

.connav {
  margin-bottom: 20rpx;
  text-align: center;
}

.bottom {
  border-radius: 80rpx;
  margin: 70rpx 50rpx;
  font-size: 35rpx;
}